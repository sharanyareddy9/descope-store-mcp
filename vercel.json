{
  "rewrites": [
    {
      "source": "/sse",
      "destination": "/api/index"
    },
    {
      "source": "/message",
      "destination": "/api/index"
    },
    {
      "source": "/.well-known/oauth-authorization-server",
      "destination": "/api/index"
    },
    {
      "source": "/authorize",
      "destination": "/api/index"
    },
    {
      "source": "/register",
      "destination": "/api/index"
    },
    {
      "source": "/health",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "functions": {
    "api/index.js": {
      "maxDuration": 30
    }
  }
}